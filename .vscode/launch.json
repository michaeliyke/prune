  "version": "0.2.0",
  "configurations": [
    {
      "name": "Prune dir",
      "type": "python",
      "request": "launch",
      "module": "pruning.dir",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "args": [
        "C:\\Users\\Iyke\\BASES\\COURSES\\cloud", "node_modules", "-d"
      ]
    },
    {
      "name": "GA",
      "type": "python",
      "request": "launch",
      "module": "pruning.ga",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "args": [
        "-d",  "user",  "mails",  "phones",  "-fF",  "-c",
        "true",  "-k", "-l",  "track", "-D", "safe",
      ]
    },
    {
      "name": "Python: file",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "D:\\Users\\Iyke\\PYENVs\\django\\securing-django-applications\\Ex\\api",
        "html",
        "--dry-run"
      ]
    },
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "pruning.prune_files",
      "justMyCode": true,
      "args": [
        "D:/Users/IykePYENVs/django/securing-django-applications/Ex",
        "html"
      ]
    }
  ]
}
